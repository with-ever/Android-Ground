<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:orientation="vertical"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:id="@+id/constraintRowBoardCellParent">
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textBoardCellTitle"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="10dp" android:layout_marginTop="5dp"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textBoardCellCompany"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="10dp"
                app:layout_constraintTop_toBottomOf="@+id/textBoardCellTitle"
                android:layout_marginTop="5dp"/>

        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textBoardCellNickName"
                app:layout_constraintTop_toBottomOf="@id/textBoardCellTitle"
                app:layout_constraintStart_toEndOf="@id/textBoardCellCompany"
                android:layout_marginTop="5dp"
                android:layout_marginStart="8dp"/>

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView2"
                app:srcCompat="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                tools:visibility="visible"/>
        <TextView
                android:text="TextViewasdasdasdasdasdasdasdaskdhjajksdhjkashdjka'ajsdhakjshdajkshdjkashdkjas'"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textBoardCellContens"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/imageView2"
                app:layout_constraintTop_toBottomOf="@id/textBoardCellCompany"
                android:layout_marginTop="8dp"
                android:layout_marginStart="10dp" android:layout_marginEnd="10dp"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textBoardCellWatchCount"
                app:layout_constraintTop_toBottomOf="@+id/textBoardCellContens"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="8dp"
                android:layout_marginStart="10dp"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textBoardCellCreateTime"
                app:layout_constraintStart_toEndOf="@+id/textBoardCellWatchCount"
                app:layout_constraintTop_toBottomOf="@id/textBoardCellContens"
                android:layout_marginTop="8dp"
                android:layout_marginStart="10dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>